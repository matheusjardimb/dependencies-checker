{
  "name": "exact-dependency-checker",
  "version": "0.8.1",
  "description": "JS exact dependency action",
  "main": "src/main.ts",
  "scripts": {
    "cli_validate": "node src/tasks/validate.ts",
    "cli": "node src/tasks/cli.ts",
    "pre-commit.install": "husky install",
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "prepare": "ncc build src/main.ts -o dist --minify",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matheusjardimb/js-exact-dependency-action"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "JavaScript",
    "Dependencies"
  ],
  "author": {
    "name": "Matheus Jardim Bernardes",
    "url": "https://matheusjardimb.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matheusjardimb/js-exact-dependency-action/issues"
  },
  "homepage": "https://github.com/matheusjardimb/js-exact-dependency-action#readme",
  "dependencies": {
    "@actions/core": "1.10.0",
    "find-duplicated-property-keys": "1.2.9"
  },
  "devDependencies": {
    "@types/find-duplicated-property-keys": "1.1.1",
    "@types/mri": "1.1.0",
    "@types/node": "18.16.3",
    "@typescript-eslint/parser": "5.59.2",
    "@vercel/ncc": "0.36.1",
    "eslint": "8.44.0",
    "eslint-plugin-github": "4.8.0",
    "eslint-plugin-jest": "27.2.2",
    "husky": "8.0.3",
    "jest": "29.6.0",
    "jest-cli": "29.5.0",
    "js-yaml": "4.1.0",
    "mri": "1.1.4",
    "prettier": "2.8.8",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  },
  "bin": "./dist/main.js"
}
