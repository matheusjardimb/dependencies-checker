{
  "main": "src/main.ts",
  "scripts": {
    "cli_validate": "node src/tasks/validate.ts",
    "cli": "node src/tasks/cli.ts",

    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "prepare": "ncc build src/main.ts -o dist --minify",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matheusjardimb/js-exact-dependency-action"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "JavaScript",
    "Dependencies"
  ],
  "author": {
    "name": "Matheus Jardim Bernardes",
    "url": "https://matheusjardimb.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matheusjardimb/js-exact-dependency-action/issues"
  },
  "homepage": "https://github.com/matheusjardimb/js-exact-dependency-action#readme",
  "bin": "./dist/main.js"
}
