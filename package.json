{
  "name": "exact-dependency-checker",
  "version": "0.4.0",
  "description": "JS exact dependency action",
  "main": "src/main.js",
  "scripts": {
    "pre-commit.install": "husky install",
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matheusjardimb/js-exact-dependency-action"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "JavaScript",
    "Dependencies"
  ],
  "author": {
    "name": "Matheus Jardim Bernardes",
    "url": "https://matheusjardimb.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matheusjardimb/js-exact-dependency-action/issues"
  },
  "homepage": "https://github.com/matheusjardimb/js-exact-dependency-action#readme",
  "dependencies": {
    "@actions/core": "1.10.0"
  },
  "devDependencies": {
    "@vercel/ncc": "0.36.1",
    "husky": "8.0.3",
    "@types/node": "18.16.3",
    "@typescript-eslint/parser": "5.59.2",
    "eslint": "8.39.0",
    "eslint-plugin-github": "4.7.0",
    "eslint-plugin-jest": "27.2.1",
    "jest": "29.5.0",
    "jest-cli": "29.5.0",
    "js-yaml": "4.1.0",
    "prettier": "2.8.8",
    "ts-jest": "29.1.0",
    "typescript": "5.0.4"
  },

  "bin": "./src/main.js"
}
