{
  "name": "js-exact-dependency-action",
  "version": "0.4.0",
  "description": "JS exact dependency action",
  "main": "dist/index.js",
  "scripts": {
    "lint": "semistandard",
    "lint.fix": "semistandard --fix",
    "prepare": "ncc build lib/index.js -o dist --minify",
    "test": "jest",
    "pre-commit.install": "husky install",
    "pre-commit.check": "npm run lint.fix && npm run test",
    "all": "npm run lint && npm run prepare && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matheusjardimb/js-exact-dependency-action.git"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "JavaScript",
    "Dependencies"
  ],
  "author": "opensource+matheusjardimb/js-exact-dependency-action@github.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matheusjardimb/js-exact-dependency-action/issues"
  },
  "homepage": "https://github.com/matheusjardimb/js-exact-dependency-action#readme",
  "dependencies": {
    "@actions/core": "1.9.1"
  },
  "devDependencies": {
    "@vercel/ncc": "0.36.1",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "jest-cli": "29.5.0",
    "semistandard": "17.0.0"
  },
  "semistandard": {
    "env": [
      "jest"
    ]
  }
}
